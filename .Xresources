Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintsfull ! hintslight | hintsmedium | hintsfull
Xft.hinting: true
Xft.antialias: true
Xft.rgba: rgb
Xft.dpi: 96

Xcursor.size: 32

! gruvbox-dark

! hard contrast: *background: #1d2021
!*background: #282828
*background: #1c1c1c
! soft contrast: *background: #32302f
*foreground: #ebdbb2
! Black + DarkGrey
*color0:  #282828
*color8:  #928374
! DarkRed + Red
*color1:  #cc241d
*color9:  #fb4934
! DarkGreen + Green
*color2:  #98971a
*color10: #b8bb26
! DarkYellow + Yellow
*color3:  #d79921
*color11: #fabd2f
! DarkBlue + Blue
*color4:  #458588
*color12: #83a598
! DarkMagenta + Magenta
*color5:  #b16286
*color13: #d3869b
! DarkCyan + Cyan
*color6:  #689d6a
*color14: #8ec07c
! LightGrey + White
*color7:  #a89984
*color15: #ebdbb2

URxvt.scrollBar: false
URxvt.internalBorder: 15

URxvt*cursorColor: #d99921
URxvt*cursorBlink: 1

URxvt*backgroundPixmap: /home/marvin/875.png;style=tiled

URxvt*font: xft:comic mono:antialias=true:size=13, xft:monaco:size=13:antialias=true
URxvt*boldFont: 
URxvt*italicFont: 
URxvt*bolditalicFont:
